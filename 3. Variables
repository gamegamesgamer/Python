#define variables#
A=34
B="Harry"
C=45.32


#print function with variables#
print(A)
print(B)
print(C)
print(A+C)
print(A*5)
print(B*4)


#check type of variable#
print(type(A))
print(type(B))
print(type(C))


#change values of variable#
D=10
D=11
print(D)
E=10
E="10"
print(E)
F=100
F=100.00
print(F)
